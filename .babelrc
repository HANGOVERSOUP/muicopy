{
  "presets": ["es2015", "stage-1", "react"],
  "plugins": [
    "./scripts/babel-transform-object-assign-require",
    "transform-dev-warning"
  ],
  "env": {
    "coverage": {
      "plugins": [
        ["istanbul"]
      ]
    },
    "test": {
      "sourceMaps": "both"
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        ["react-remove-properties", {"properties": ["data-mui-test"]}]
      ]
    }
  }
}
