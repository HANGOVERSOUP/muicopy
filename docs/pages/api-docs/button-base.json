      "required": false,
      "default": "0"
    },
    "TouchRippleProps": { "type": { "name": "object" }, "required": false },
    "type": {
      "type": {
        "name": "union",
        "value": [
          {
            "name": "enum",
            "value": [
              { "value": "'button'", "computed": false },
              { "value": "'reset'", "computed": false },
              { "value": "'submit'", "computed": false }
            ]
          },
          { "name": "string" }
        ]
      },
      "required": false,
      "description": "@ignore"
    }
  },
  "spread": true,
  "styles": {
    "name": "MuiButtonBase",
    "globalClasses": {
      "root": "MuiButtonBase-root",
      "disabled": "Mui-disabled",
      "focusVisible": "Mui-focusVisible"
    }
