    "onKeyUp": { "type": { "name": "func" }, "required": false, "description": "@ignore" },
    "onMouseDown": { "type": { "name": "func" }, "required": false, "description": "@ignore" },
    "onMouseLeave": { "type": { "name": "func" }, "required": false, "description": "@ignore" },
    "onMouseUp": { "type": { "name": "func" }, "required": false, "description": "@ignore" },
    "onTouchEnd": { "type": { "name": "func" }, "required": false, "description": "@ignore" },
