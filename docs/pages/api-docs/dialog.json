{
  "name": "Dialog",
  "filename": "/packages/material-ui/src/Dialog/Dialog.js",
  "description": "Dialogs are overlaid modal paper based components with a backdrop.",
  "props": {
    "aria-describedby": { "type": { "name": "string" }, "required": false },
    "aria-labelledby": { "type": { "name": "string" }, "required": false },
    "BackdropProps": { "type": { "name": "object" }, "required": false, "description": "@ignore" },
    "children": { "type": { "name": "node" }, "required": false },
    "classes": { "type": { "name": "object" }, "required": false },
    "className": { "type": { "name": "string" }, "required": false, "description": "@ignore" },
    "disableBackdropClick": { "type": { "name": "bool" }, "required": false, "default": "false" },
    "disableEscapeKeyDown": { "type": { "name": "bool" }, "required": false, "default": "false" },
    "fullScreen": { "type": { "name": "bool" }, "required": false, "default": "false" },
    "fullWidth": { "type": { "name": "bool" }, "required": false, "default": "false" },
    "maxWidth": {
      "type": {
        "name": "enum",
        "description": "'lg'<br>&#124;&nbsp;'md'<br>&#124;&nbsp;'sm'<br>&#124;&nbsp;'xl'<br>&#124;&nbsp;'xs'<br>&#124;&nbsp;false"
      },
      "required": false,
      "default": "'sm'"
    },
    "onBackdropClick": { "type": { "name": "func" }, "required": false },
    "onClose": { "type": { "name": "func" }, "required": false },
    "onEscapeKeyDown": { "type": { "name": "func" }, "required": false },
    "open": { "type": { "name": "bool" }, "required": true },
    "PaperComponent": { "type": { "name": "elementType" }, "required": false, "default": "Paper" },
    "PaperProps": { "type": { "name": "object" }, "required": false, "default": "{}" },
    "scroll": {
      "type": { "name": "enum", "description": "'body'<br>&#124;&nbsp;'paper'" },
      "required": false,
      "default": "'paper'"
    },
    "TransitionComponent": {
      "type": { "name": "elementType" },
      "required": false,
      "default": "Fade"
    },
    "transitionDuration": {
      "type": {
        "name": "union",
        "description": "number<br>&#124;&nbsp;{ appear?: number, enter?: number, exit?: number }"
      },
      "required": false,
      "default": "{ enter: duration.enteringScreen, exit: duration.leavingScreen }"
    },
    "TransitionProps": { "type": { "name": "object" }, "required": false }
  },
  "spread": true,
  "styles": {
    "name": "MuiDialog",
    "globalClasses": {
      "root": "MuiDialog-root",
      "scrollPaper": "MuiDialog-scrollPaper",
      "scrollBody": "MuiDialog-scrollBody",
      "container": "MuiDialog-container",
      "paper": "MuiDialog-paper",
      "paperScrollPaper": "MuiDialog-paperScrollPaper",
      "paperScrollBody": "MuiDialog-paperScrollBody",
      "paperWidthFalse": "MuiDialog-paperWidthFalse",
      "paperWidthXs": "MuiDialog-paperWidthXs",
      "paperWidthSm": "MuiDialog-paperWidthSm",
      "paperWidthMd": "MuiDialog-paperWidthMd",
      "paperWidthLg": "MuiDialog-paperWidthLg",
      "paperWidthXl": "MuiDialog-paperWidthXl",
      "paperFullWidth": "MuiDialog-paperFullWidth",
      "paperFullScreen": "MuiDialog-paperFullScreen"
    }
  },
  "forwardsRefTo": "HTMLDivElement",
  "inheritance": { "component": "Modal", "pathname": "/api/modal/" },
  "demos": "- [Dialogs](/components/dialogs/)"
}
