      "type": {
        "name": "enum",
        "description": "'down'<br>&#124;&nbsp;'left'<br>&#124;&nbsp;'right'<br>&#124;&nbsp;'up'"
      },
      "required": false,
      "jsdocDefaultValue": { "value": "'down'" },
