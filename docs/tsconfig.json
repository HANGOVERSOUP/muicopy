{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "ES2020",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "incremental": true
  },
  "include": ["next-env.d.ts", "types", "src", "pages", "data"],
  "exclude": ["node_modules", "export"]
}
