{
  "private": true,
  "name": "test",
  "version": "5.0.0-alpha.27",
  "scripts": {
    "karma": "cross-env NODE_ENV=test karma start karma.conf.js",
    "karma:profile": "cross-env NODE_ENV=test karma start karma.conf.profile.js",
    "regressions": "cross-env NODE_ENV=production yarn workspace test regressions:build && concurrently --success first --kill-others \"yarn workspace test regressions:run\" \"yarn workspace test regressions:server\"",
    "regressions:build": "webpack --config regressions/webpack.config.js",
    "regressions:dev": "concurrently \"yarn workspace test regressions:build --watch\" \"yarn workspace test regressions:server\"",
    "regressions:run": "mocha --config regressions/.mocharc.js --delay 'regressions/**/*.test.js'",
    "regressions:server": "serve regressions",
    "serve": "^11.3.2",
    "typescript": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "html-webpack-plugin": "^4.5.0",
    "karma": "^5.0.1",
    "karma-browserstack-launcher": "~1.4.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "raw-loader": "^1.0.0",
    "serve": "^11.3.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.0.0"
  }
}
